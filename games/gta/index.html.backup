                const searchData = card.getAttribute('data-search').toLowerCase();
                const cardTitle = card.querySelector('h3').textContent.toLowerCase();
                const cardDescription = card.querySelector('p').textContent.toLowerCase();

                const isMatch = searchTerm === '' ||
                              searchData.includes(searchTerm) ||
                              cardTitle.includes(searchTerm) ||
                              cardDescription.includes(searchTerm);

                card.style.display = isMatch ? 'flex' : 'none';
            });
        });

        // Show info functions for Lua scripts
        function showInfo(type) {
            let message = '';
            switch(type) {
                case 'kiddions-lua':
                    message = 'Kiddions Lua Scripts include:\n Money Scripts\n Vehicle Spawners\n Weapon Scripts\n Teleport Scripts\n And many more!\n\nCheck our Discord for the latest scripts.';
                    break;
                case 'yim-lua':
                    message = 'YimMenu Lua Scripts include:\n Advanced Protection Scripts\n Custom Heist Scripts\n Business Manager Scripts\n Griefing Protection\n And many more!\n\nCheck our Discord for the latest scripts.';
                    break;
                case 'stand-lua':
                    message = 'Stand Lua Scripts include:\n Advanced Features\n Custom Protections\n Unique Functions\n Enhanced Gameplay\n And many more!\n\nCheck our Discord for the latest scripts.';
                    break;
            }
            alert(message);
        }

        // Image Modal Functions
        function openImageModal(src) {
            const modal = document.getElementById('imageModal');
            const modalImg = document.getElementById('modalImage');
            modal.style.display = 'block';
            modalImg.src = src;

            // Close modal when clicking outside the image
            modal.onclick = function(event) {
                if (event.target == modal) {
                    closeImageModal();
                }
            }
        }

        function closeImageModal() {
            document.getElementById('imageModal').style.display = 'none';
        }

        // Keyboard support for modal
        document.addEventListener('keydown', function(event) {
            if (event.key === 'Escape') {
                closeImageModal();
            }
        });
    </script>

    <!-- Image Modal -->
    <div id="imageModal" class="modal">
        <span class="modal-close" onclick="closeImageModal()">&times;</span>
        <div class="modal-content">
            <img id="modalImage" src="" alt="Full Size Image">
        </div>
    </div>

